source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
use_frameworks!

target 'caicaiSwift' do
  pod 'Alamofire', '~> 3.3'
  pod 'EasyPeasy'   #is a Swift framework that lets you create Auto Layout constraints programmatically without headaches and never ending boilerplate code
  pod 'FBRetainCycleDetector'    #iOS library to help detecting retain cycles in runtime.
  pod 'Rollout.io', '~> 0.15.0'
end
